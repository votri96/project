<div class="latest-articles">
  <div class="world-news-grids">
    <% @tagdetails.each do |tagdetail| %>
    <% tin = tagdetail.tin %>
      <div class="world-news-grid">
      <a href="<%= tin_path(tin) %>"><%= image_tag tin.image.url, class: "img-responsive" %></a>
      <a href="<%= tin_path(tin) %>" class="wd"><%= tin.name %></a>
      <a href="<%= tin_path(tin) %>"><p><%= tin.title %></p></a>
      <a class="read" href="<%= tin_path(tin) %>">Read More</a>
    </div>
    <% end %>
    <div class="clearfix"></div>
    <%= paginate @tagdetails %>
    </div>
</div>
  