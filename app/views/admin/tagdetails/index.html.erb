<p id="notice"><%= notice %></p>

<h1>Tagdetails</h1>

<table class = "table table-bordered table-striped table-condensed table-responsive">
  <thead>
    <tr>
      <th>Tag</th>
      <th>Tin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tagdetails.each do |tagdetail| %>
      <tr>
        <td><%= tagdetail.tag.name %></td>
        <td><%= tagdetail.tin.name %></td>
        <td><%= link_to 'Show', admin_tagdetail_path(tagdetail), class: "btn-sm btn-primary" %></td>
        <td><%= link_to 'Edit', edit_admin_tagdetail_path(tagdetail), class: "btn-sm btn-primary" %></td>
        <td><%= link_to 'Destroy', admin_tagdetail_path(tagdetail), class: "btn-sm btn-primary" , method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @tagdetails %>
<br>

<%= link_to 'New Tagdetail', new_admin_tagdetail_path, class: "btn btn-success"  %>
