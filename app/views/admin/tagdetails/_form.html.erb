
<%= simple_form_for  [:admin, @tagdetail] do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.association :tag %>
    <%= f.association :tin %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
